		"charts",
		"angular",
		"dataviz",
		"data-visualization",
		"visualizations",
		"d3",
		"svg",
		"component",
		"components",
		"css",
		"html",
		"ibm",
		"typescript",
		"javascript",
		"js",
		"library",
		"pattern",
		"patterns",
		"sass",
		"scss"
	],
	"dependencies": {
		"@ibm-sterling/charts": "^0.23.1"
	},
